<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUITest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUITest"
    Shell.FlyoutBehavior="Disabled"
    xmlns:pages="clr-namespace:MAUITest.Pages"
    xmlns:>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Alle Aufgaben"
        ContentTemplate="{DataTemplate pages:ToDoListPage}"
        Route="ToDoListPage" />
    
    <ShellContent
    Title="Alle Aufgaben"
    ContentTemplate="{DataTemplate pages:ToDoListPage}"
    Route="ToDoListPage" />
    
    <ShellContent
    Title="Alle Aufgaben"
    ContentTemplate="{DataTemplate pages:ToDoListPage}"
    Route="ToDoListPage" />
</Shell>
