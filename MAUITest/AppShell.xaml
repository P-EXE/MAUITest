<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUITest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:MAUITest.Pages"
    xmlns:local="clr-namespace:MAUITest"
    Shell.FlyoutBehavior="Flyout">

  <ShellContent
        Title="Alle"
        ContentTemplate="{DataTemplate pages:ToDoListPage}"
        Route="ToDoListPage" />
  <ShellContent
        Title="Neu"
        ContentTemplate="{DataTemplate pages:ToDoDetailsPage}"
        Route="ToDoDetailsPage" />
</Shell>
